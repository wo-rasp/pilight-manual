     <text:p text:style-name="P42"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc154_547078094" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T59">API<text:tab/>1</text:span></text:a></text:p>
     <text:p text:style-name="P43"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc156_547078094" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T59">Identifizierung<text:tab/>1</text:span></text:a></text:p>
     <text:p text:style-name="P43"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc158_547078094" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T59">Optionen<text:tab/>2</text:span></text:a></text:p>
     <text:p text:style-name="P43"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc160_547078094" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T59">Aktionen<text:tab/>3</text:span></text:a></text:p>
     <text:p text:style-name="P43"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc162_547078094" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link"><text:span text:style-name="T59">Webserver<text:tab/>6</text:span></text:a></text:p>
